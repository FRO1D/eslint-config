{
	"name": "@morev/eslint-config",
	"description": "Strict shareable ESLint configuration that I personally use",
	"version": "0.2.4",
	"private": false,
	"engines": {
		"node": "^12.20.0 || ^14.13.1 || >=16.0.0"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"author": {
		"name": "Maxim Morev",
		"email": "max.seainside@gmail.com"
	},
	"homepage": "https://github.com/MorevM/eslint-config-more#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/MorevM/eslint-config-more"
	},
	"keywords": [
		"eslint",
		"eslintconfig",
		"config",
		"codestyle",
		"more"
	],
	"files": [
		"configurations",
		"utils",
		".eslintrc.js"
	],
	"exports": {
		".": "./.eslintrc.js",
		"./base": "./build/base.js",
		"./base/warn-autofixable": "./build/base-warn-autofixable.js",
		"./base/off-autofixable": "./build/base-off-autofixable.js",
		"./jest": "./build/jest.js",
		"./jest/warn-autofixable": "./build/jest-warn-autofixable.js",
		"./jest/off-autofixable": "./build/jest-off-autofixable.js",
		"./json": "./build/json.js",
		"./json/warn-autofixable": "./build/json-warn-autofixable.js",
		"./json/off-autofixable": "./build/json-off-autofixable.js",
		"./jsonc": "./build/jsonc.js",
		"./jsonc/warn-autofixable": "./build/jsonc-warn-autofixable.js",
		"./jsonc/off-autofixable": "./build/jsonc-off-autofixable.js",
		"./package-json": "./build/package-json.js",
		"./package-json/warn-autofixable": "./build/package-json-warn-autofixable.js",
		"./package-json/off-autofixable": "./build/package-json-off-autofixable.js",
		"./vue2": "./build/vue2.js",
		"./vue2/warn-autofixable": "./build/vue2-warn-autofixable.js",
		"./vue2/off-autofixable": "./build/vue2-off-autofixable.js",
		"./yaml": "./build/yaml.js",
		"./yaml/warn-autofixable": "./build/yaml-warn-autofixable.js",
		"./yaml/off-autofixable": "./build/yaml-off-autofixable.js"
	},
	"main": "./.eslintrc.js",
	"scripts": {
		"prepare": "husky install ./.husky",
		"build": "node utils/build.js",
		"test": "jest",
		"lint": "eslint \"**/*.{js,json,json5,jsonc,yml,yaml}\"",
		"lint:fix": "eslint \"**/*.{js,json,json5,jsonc,yml,yaml}\" --fix",
		"release": "release-it --disable-metrics"
	},
	"peerDependencies": {
		"eslint": "^8.7.0"
	},
	"dependencies": {
		"@babel/core": "^7.16.12",
		"@babel/eslint-parser": "^7.16.5",
		"eslint-plugin-jest": "^26.0.0",
		"eslint-plugin-jest-formatting": "^3.1.0",
		"eslint-plugin-jsonc": "^2.0.0",
		"eslint-plugin-no-autofix": "^1.2.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-sonarjs": "^0.11.0",
		"eslint-plugin-unicorn": "^40.0.1",
		"eslint-plugin-vue": "^8.2.0",
		"eslint-plugin-yml": "^0.12.0",
		"jsonc-eslint-parser": "^2.1.0",
		"lodash.clonedeep": "^4.5.0",
		"lodash.mergewith": "^4.6.2",
		"vue-eslint-parser": "^8.1.0",
		"yaml-eslint-parser": "^0.5.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.1.0",
		"@release-it/conventional-changelog": "^4.1.0",
		"@types/jest": "^27.4.0",
		"eslint": "^8.7.0",
		"husky": "^7.0.4",
		"jest": "^27.4.7",
		"lint-staged": "^12.2.2",
		"release-it": "^14.12.3"
	}
}
